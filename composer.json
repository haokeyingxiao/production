{
  "name": "haokeyingxiao/production",
  "license": "Apache-2.0",
  "type": "project",
  "require":
  {
    "composer-runtime-api": "^2.0",
    "haokeyingxiao/administration": "*",
    "haokeyingxiao/core": "v6.6.4.3",
    "haokeyingxiao/elasticsearch": "*",
    "haokeyingxiao/storefront": "*",
    "symfony/flex": "~2"
  },
  "repositories":
  [
    {
      "type": "path",
      "url": "custom/plugins/*",
      "options":
      {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "custom/plugins/*/packages/*",
      "options":
      {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "custom/static-plugins/*",
      "options":
      {
        "symlink": true
      }
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config":
  {
    "allow-plugins":
    {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts":
  {
    "auto-scripts": [],
    "post-install-cmd":
    [
      "@auto-scripts"
    ],
    "post-update-cmd":
    [
      "@auto-scripts"
    ]
  },
  "extra":
  {
    "symfony":
    {
      "allow-contrib": true,
      "endpoint":
      [
        "https://raw.githubusercontent.com/haokeyingxiao/recipes/flex/main/index.json",
        "flex://defaults"
      ]
    }
  }
}